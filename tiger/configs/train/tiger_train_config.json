{
  "experiment_name": "tiger_beauty",
  "dataset": {
    "inter_json_path": "../data/Beauty/inter.json",
    "max_sequence_length": 20,
    "sampler_type": "last_item_prediction"
  },
  "index_json_path": "../data/Beauty/index.json",
  "dataloader_batch_size": {
    "train": 256,
    "validation": 1024
  },
  "model": {
    "embedding_dim": 384,
    "codebook_size": 256,
    "num_positions": 20,
    "num_heads": 6,
    "num_encoder_layers": 4,
    "num_decoder_layers": 4,
    "dim_feedforward": 2048,
    "dropout": 0.1,
    "initializer_range": 0.02
  },
  "optimizer": {
    "type": "adam",
    "lr": 3e-4
  }
}