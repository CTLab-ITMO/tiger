{
  "experiment_name": "sasrec_beauty_letter",
  "best_metric": "validation/ndcg@20",
  "dataset": {
    "path_to_data_dir": "../data",
    "name": "Beauty",
    "max_sequence_length": 20,
    "sampler_type": "next_item_prediction"
  },
  "dataloader_batch_size": {
    "train": 256,
    "validation": 512
  },
  "model": {
    "embedding_dim": 64,
    "num_heads": 2,
    "num_layers": 2,
    "dim_feedforward": 256,
    "dropout": 0.3,
    "activation": "relu",
    "layer_norm_eps": 1e-9,
    "initializer_range": 0.02
  },
  "optimizer": {
    "type": "adam",
    "lr": 0.001
  }
}