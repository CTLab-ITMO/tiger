{
  "experiment_name": "sasrec_beauty",
  "dataset": {
    "inter_json_path": "../data/Beauty/inter.json",
    "max_sequence_length": 20,
    "sampler_type": "next_item"
  },
  "dataloader": {
    "train_batch_size": 256,
    "validation_batch_size": 512
  },
  "model": {
    "embedding_dim": 64,
    "num_heads": 2,
    "num_layers": 2,
    "dim_feedforward": 256,
    "dropout": 0.3,
    "activation": "relu",
    "layer_norm_eps": 1e-9,
    "initializer_range": 0.02
  },
  "optimizer": {
    "type": "adam",
    "lr": 0.001
  }
}